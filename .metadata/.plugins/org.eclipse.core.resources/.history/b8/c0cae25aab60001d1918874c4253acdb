package pattern;

public class HomePattern {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int n=8;
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<n;j++)
			{
				if((j==0 || j==1 || j==n-2 || j==n-1) || i==n)
				{
				System.out.print("*");
				}
				else
				{
				System.out.print(" ");
				}
				
			}
			System.out.println();
		}
	}
}
